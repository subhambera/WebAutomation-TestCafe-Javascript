{
  "dependencies": {
    "prettier": "^2.8.0",
    "testcafe": "^3.6.2",
    "xpath-to-css": "^1.1.0"
  },
  "scripts": {
    "test:chrome": "testcafe chrome ./tests -s takeOnFails=true",
    "test:chrome:5": "testcafe -c 5 chrome ./tests -s takeOnFails=true",
    "test:chrome:headless": "testcafe chrome:headless ./tests -s takeOnFails=true",
    "test:chrome:mobile": "testcafe chrome:emulation:device=iphone X ./tests -s takeOnFails=true",
    "test:edge": "testcafe edge ./tests -s takeOnFails=true",
    "test:edge:headless": "testcafe edge:headless ./tests/ZeroBankTesting -s takeOnFails=true",
    "test:firefox": "testcafe firefox ./tests -s takeOnFails=true",
    "test:multiple": "testcafe chrome,edge ./tests -s takeOnFails=true"
  },
  "name": "testcafetesting",
  "version": "1.0.0",
  "description": "Automated testing with TestCafe",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "TestCafe",
    "Testing",
    "Automation"
  ],
  "author": "SubhamBera",
  "license": "ISC"
}
